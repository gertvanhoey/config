{
    "title": "Programming on AZERTY",
    "rules": [
      {
        "description": "right_option+^ for {",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "open_bracket",
              "modifiers": {
                "mandatory": [
                  "right_alt"
                ]
              }
            },
            "to": [
              {
                "key_code": "5",
                "modifiers": [
                  "right_alt"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "right_option+$ for }",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "close_bracket",
              "modifiers": {
                "mandatory": [
                  "right_alt"
                ]
              }
            },
            "to": [
              {
                "key_code": "hyphen",
                "modifiers": [
                  "right_alt"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "right_option+ù for [",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "quote",
              "modifiers": {
                "mandatory": [
                  "right_alt"
                ]
              }
            },
            "to": [
              {
                "key_code": "5",
                "modifiers": [
                  "right_alt",
                  "right_shift"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "right_option+` for ]",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "backslash",
              "modifiers": {
                "mandatory": [
                  "right_alt"
                ]
              }
            },
            "to": [
              {
                "key_code": "hyphen",
                "modifiers": [
                  "right_alt",
                  "right_shift"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "right_option+e for €",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "e",
              "modifiers": {
                "mandatory": [
                  "right_alt"
                ]
              }
            },
            "to": [
              {
                "key_code": "close_bracket",
                "modifiers": [
                  "right_alt"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "left_option+e for €",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "e",
              "modifiers": {
                "mandatory": [
                  "left_alt"
                ]
              }
            },
            "to": [
              {
                "key_code": "close_bracket",
                "modifiers": [
                  "right_alt"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "right_option+: for \\",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "period",
              "modifiers": {
                "mandatory": [
                  "right_alt"
                ]
              }
            },
            "to": [
              {
                "key_code": "period",
                "modifiers": [
                  "right_alt",
                  "right_shift"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "à for (",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "0"
            },
            "to": [
              {
                "key_code": "5"
              }
            ]
          }
        ]
      },
      {
        "description": "^ without spacebar",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "open_bracket"
            },
            "to": [
              {
                "key_code": "open_bracket"
              },
              {
                "key_code": "spacebar"
              }
            ]
          }
        ]
      },
      {
        "description": "right_option+= for ~",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "slash",
              "modifiers": {
                "mandatory": [
                  "right_alt"
                ]
              }
            },
            "to": [
              {
                "key_code": "n",
                "modifiers": [
                  "right_alt"
                ]
              },
              {
                "key_code": "spacebar"
              }
            ]
          }
        ]
      }
    ]
  }
  